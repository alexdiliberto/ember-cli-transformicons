{
  "name": "ember-cli-transformicons",
  "version": "1.0.2",
  "description": "Transformicons addon for your Ember CLI project",
  "keywords": [
    "ember-addon",
    "ember",
    "ember-cli",
    "transformicon",
    "navicon",
    "icon",
    "transform",
    "animation"
  ],
  "license": "MIT",
  "author": "Alex DiLiberto <npm@alexdiliberto.com>",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/alexdiliberto/ember-cli-transformicons"
  },
  "bugs": {
    "url": "https://github.com/alexdiliberto/ember-cli-transformicons/issues"
  },
  "scripts": {
    "postinstall": "npm rebuild node-sass",
    "build": "ember build",
    "start": "ember server",
    "test": "ember try:each",
    "addon-tests": "mocha test/*-test.js",
    "test:all": "ember try:each && mocha test/*-test.js",
    "test:iterate": "ember exam:iterate 10",
    "start:inspect": "node --inspect --debug-brk node_modules/.bin/ember s",
    "build:stats": "CONCAT_STATS=true ember b"
  },
  "dependencies": {
    "ember-cli-babel": "^6.4.1",
    "ember-cli-htmlbars": "^2.0.2",
    "eslint-plugin-ember": "^3.4.1"
  },
  "devDependencies": {
    "broccoli-asset-rev": "^2.4.5",
    "chai": "^4.0.2",
    "chalk": "^2.0.1",
    "ember-ajax": "^3.0.0",
    "ember-cli": "2.13.3",
    "ember-cli-addon-tests": "~0.7.0",
    "ember-cli-anybar": "0.5.0",
    "ember-cli-content-security-policy": "0.6.1",
    "ember-cli-dependency-checker": "^2.0.0",
    "ember-cli-dependency-lint": "1.0.2",
    "ember-cli-eslint": "^4.0.0",
    "ember-cli-github-pages": "0.1.2",
    "ember-cli-htmlbars-inline-precompile": "^0.4.3",
    "ember-cli-inject-live-reload": "^1.4.1",
    "ember-cli-qunit": "^4.0.0",
    "ember-cli-sass": "^7.0.0",
    "ember-cli-shims": "^1.1.0",
    "ember-cli-sri": "^2.1.0",
    "ember-cli-uglify": "^1.2.0",
    "ember-disable-prototype-extensions": "^1.1.2",
    "ember-exam": "^0.7.0",
    "ember-export-application-global": "^2.0.0",
    "ember-load-initializers": "^1.0.0",
    "ember-native-dom-helpers": "0.5.1",
    "ember-percy": "1.2.13",
    "ember-resolver": "^4.0.0",
    "ember-source": "~2.13.3",
    "loader.js": "4.5.1",
    "mocha": "^3.4.2",
    "request": "^2.81.0",
    "rsvp": "^3.5.0"
  },
  "engines": {
    "node": "^4.5 || 6.* || >= 7.*"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "demoURL": "http://alexdiliberto.com/ember-cli-transformicons"
  }
}
