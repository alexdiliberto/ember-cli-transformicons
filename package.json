{
  "name": "ember-transformicons",
  "version": "2.2.0",
  "description": "Transformicons for Ember: Animated icons, symbols and buttons",
  "keywords": [
    "ember-addon",
    "ember",
    "ember-cli",
    "transformicon",
    "navicon",
    "icon",
    "transform",
    "animation"
  ],
  "repository": "https://github.com/alexdiliberto/ember-transformicons",
  "license": "MIT",
  "author": "Alex DiLiberto <npm@alexdiliberto.com>",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "homepage": "https://alexdiliberto.github.io/ember-transformicons",
  "bugs": {
    "url": "https://github.com/alexdiliberto/ember-transformicons/issues"
  },
  "scripts": {
    "postinstall": "npm rebuild node-sass",
    "build": "ember build",
    "build:stats": "CONCAT_STATS=true yarn build",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint .",
    "lint:fix": "yarn lint -- --fix",
    "start": "ember serve",
    "start:inspect": "node --inspect-brk node_modules/.bin/ember s",
    "test": "ember test",
    "test:all": "ember try:each && ember fastboot:test",
    "test:iterate": "ember exam:iterate 10",
    "test:fastboot": "DEBUG=ember-cli-addon-tests ember fastboot:test"
  },
  "dependencies": {
    "@ember-decorators/babel-transforms": "^2.0.2",
    "ember-auto-import": "^1.2.15",
    "ember-cli-babel": "^6.6.0",
    "ember-cli-htmlbars": "^2.0.2",
    "ember-decorators": "^3.0.0",
    "ember-legacy-class-shim": "^1.0.5",
    "lodash.defaultto": "^4.14.0"
  },
  "devDependencies": {
    "@alexdiliberto/eslint-config": "^2.1.0",
    "@ember/optional-features": "^0.6.4",
    "babel-eslint": "^8.0.0",
    "broccoli-asset-rev": "^2.7.0",
    "broccoli-funnel": "^2.0.1",
    "broccoli-merge-trees": "^3.0.1",
    "broccoli-source": "^1.1.0",
    "chai": "^4.1.2",
    "denodeify": "^1.2.1",
    "ember-ajax": "^3.0.0",
    "ember-angle-bracket-invocation-polyfill": "^1.2.2",
    "ember-cli": "~3.3.0",
    "ember-cli-addon-docs": "^0.5.3",
    "ember-cli-addon-docs-esdoc": "^0.2.1",
    "ember-cli-addon-tests": "^0.11.0",
    "ember-cli-dependency-checker": "^2.1.1",
    "ember-cli-dependency-lint": "1.1.1",
    "ember-cli-deploy": "^1.0.2",
    "ember-cli-deploy-build": "^1.1.1",
    "ember-cli-deploy-git": "^1.3.3",
    "ember-cli-deploy-git-ci": "^1.0.1",
    "ember-cli-eslint": "^4.2.3",
    "ember-cli-fastboot": "^1.1.4-beta.1",
    "ember-cli-github-pages": "0.2.0",
    "ember-cli-htmlbars-inline-precompile": "^1.0.3",
    "ember-cli-inject-live-reload": "^1.7.0",
    "ember-cli-qunit": "^4.3.2",
    "ember-cli-sass": "^7.1.3",
    "ember-cli-shims": "^1.2.0",
    "ember-cli-sri": "^2.1.0",
    "ember-cli-template-lint": "^1.0.0-beta.1",
    "ember-cli-uglify": "^2.0.0",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-exam": "^1.0.0",
    "ember-export-application-global": "^2.0.0",
    "ember-fastboot-addon-tests": "^0.4.0",
    "ember-fetch": "4.0.2",
    "ember-load-initializers": "^1.1.0",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-native-dom-helpers": "^0.6.2",
    "ember-open-browser": "^1.0.0",
    "ember-percy": "1.5.0",
    "ember-resolver": "^4.3.0",
    "ember-source": "~3.3.0",
    "ember-source-channel-url": "^1.0.1",
    "ember-try": "^0.2.23",
    "eslint-plugin-ember": "^5.2.0",
    "eslint-plugin-node": "^6.0.1",
    "loader.js": "4.7.0",
    "qunit-dom": "^0.8.0"
  },
  "engines": {
    "node": "6.* || 8.* || >= 10.*"
  },
  "greenkeeper": {
    "ignore": [
      "@ember/optional-features",
      "broccoli-asset-rev",
      "ember-ajax",
      "ember-cli",
      "ember-cli-dependency-checker",
      "ember-cli-eslint",
      "ember-cli-htmlbars",
      "ember-cli-htmlbars-inline-precompile",
      "ember-cli-inject-live-reload",
      "ember-load-initializers",
      "ember-resolver",
      "ember-source",
      "ember-source-channel-url",
      "ember-try",
      "loader.js"
    ]
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "demoURL": "https://alexdiliberto.com/ember-transformicons",
    "versionCompatibility": {
      "ember": ">=1.12.2"
    }
  }
}
