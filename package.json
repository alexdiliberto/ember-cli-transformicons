{
  "name": "ember-transformicons",
  "version": "6.0.5",
  "description": "Transformicons for Ember: Animated icons, symbols and buttons",
  "keywords": [
    "ember-addon",
    "ember",
    "ember-cli",
    "transformicon",
    "navicon",
    "icon",
    "transform",
    "animation"
  ],
  "repository": "https://github.com/alexdiliberto/ember-transformicons",
  "license": "MIT",
  "author": "Alex DiLiberto <npm@alexdiliberto.com>",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "homepage": "https://alexdiliberto.github.io/ember-transformicons",
  "bugs": {
    "url": "https://github.com/alexdiliberto/ember-transformicons/issues"
  },
  "scripts": {
    "analyze": "broccoli-concat-analyser ./concat-stats-for",
    "build": "ember build",
    "build:prod": "ember build -prod",
    "build:prod:stats": "CONCAT_STATS=true yarn build:prod",
    "clean": "rm -rf dist tmp node_modules concat-stats-for",
    "changelog": "lerna-changelog",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint . --ext .ts --ext .js",
    "lint:dependency": "ember dependency-lint",
    "lint:types": "tsc --skipLibCheck",
    "release": "release-it",
    "start": "ember serve",
    "start:inspect": "node --inspect-brk yarn start",
    "test": "npm-run-all lint:* test:*",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each",
    "prepublishOnly": "ember ts:precompile",
    "postpublish": "ember ts:clean"
  },
  "dependencies": {
    "@glimmer/component": "^1.0.0",
    "ember-auto-import": "^1.5.3",
    "ember-cli-babel": "^7.18.0",
    "ember-cli-htmlbars": "^5.1.1",
    "ember-cli-typescript": "^3.1.2",
    "ember-decorators": "^6.1.1",
    "ember-truth-helpers": "^2.1.0"
  },
  "devDependencies": {
    "@alexdiliberto/eslint-config": "^4.1.1",
    "@ember/edition-utils": "^1.2.0",
    "@ember/optional-features": "^1.3.0",
    "@types/ember": "^3.1.1",
    "@types/ember-qunit": "^3.4.7",
    "@types/ember__test-helpers": "^1.7.2",
    "@types/qunit": "^2.9.0",
    "@types/rsvp": "^4.0.3",
    "@typescript-eslint/eslint-plugin": "^3.9.1",
    "@typescript-eslint/parser": "^3.9.0",
    "broccoli-asset-rev": "^3.0.0",
    "broccoli-concat-analyser": "^4.3.5",
    "ember-cli": "~3.17.0",
    "ember-cli-addon-docs": "^0.8.0",
    "ember-cli-addon-docs-yuidoc": "^0.2.3",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-dependency-lint": "^1.1.3",
    "ember-cli-deploy": "^1.0.2",
    "ember-cli-deploy-build": "^2.0.0",
    "ember-cli-deploy-git": "^1.3.4",
    "ember-cli-deploy-git-ci": "^1.0.1",
    "ember-cli-favicon": "^2.2.0",
    "ember-cli-inject-live-reload": "^2.0.2",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-typescript-blueprints": "^3.0.0",
    "ember-cli-uglify": "^3.0.0",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.1",
    "ember-load-initializers": "^2.1.1",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-open-browser": "^1.0.0",
    "ember-percy": "1.6.0",
    "ember-qunit": "^4.6.0",
    "ember-resolver": "^8.0.0",
    "ember-source": "~3.20.4",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^2.4.0",
    "ember-try": "^1.4.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-ember": "^8.3.0",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-qunit": "^4.0.0",
    "lerna-changelog": "^1.0.0",
    "loader.js": "4.7.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.2",
    "qunit-dom": "^1.1.0",
    "release-it": "^13.1.1",
    "release-it-lerna-changelog": "^2.1.0",
    "typescript": "3.9.7"
  },
  "engines": {
    "node": "10.* || >= 12"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "demoURL": "https://alexdiliberto.com/ember-transformicons",
    "versionCompatibility": {
      "ember": ">=3.13"
    }
  },
  "release-it": {
    "plugins": {
      "release-it-lerna-changelog": {
        "infile": "CHANGELOG.md",
        "launchEditor": true
      }
    },
    "git": {
      "tagName": "v${version}"
    },
    "github": {
      "release": true
    },
    "npm": {
      "publish": true
    }
  },
  "volta": {
    "node": "12.16.1",
    "yarn": "1.22.4"
  }
}
